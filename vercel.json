{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/camunda-worker",
      "dest": "src/index.js",
      "methods": ["GET"]
    }
  ],
  "crons": [
    {
      "path": "/camunda-worker",
      "schedule": "*/1 * * * *"
    }
  ]
}
